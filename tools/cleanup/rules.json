{
  "exclude": ["node_modules/**",".git/**","archive/**",".venv/**","venv/**"],
  "buildArtifacts": ["**/dist/**","**/build/**","**/coverage/**","**/.pytest_cache/**","**/.mypy_cache/**","**/.ruff_cache/**","**/playwright-report/**","**/.playwright/**","**/.cache/**"],
  "testArtifacts": ["**/__snapshots__/**","**/reports/**","**/screenshots/**","**/videos/**","**/junit*.xml","**/allure-results/**","**/artifacts/**"],
  "mustKeep": ["**/src/**","**/*.ts","**/*.tsx","**/*.js","**/*.py","**/*.go","**/go.mod","**/pyproject.toml","**/requirements*.txt","**/package.json","**/tsconfig*.json","**/.github/**","**/schemas/**","**/docs/**","**/README*","**/LICENSE*"],
  "archiveTargets": {
    "tests": "archive/tests",
    "builds": "archive/builds"
  },
  "safety": {
    "safeModeDefault": true,
    "forcePurgeEnv": "FORCE_PURGE"
  }
}
