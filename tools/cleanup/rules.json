{
  "archiveTargets": [
    { "from": "tests/fixtures", "to": "archive/tests/fixtures" },
    { "from": "tests/.artifacts", "to": "archive/artifacts/_dot" },
    { "from": "metrics", "to": "archive/tests/metrics" },
    { "from": "results/ab", "to": "archive/tests/results/ab" },
    { "from": "results/hybrid", "to": "archive/tests/results/hybrid" },
    { "from": "artifacts", "to": "archive/artifacts" },
    { "from": ".artifacts", "to": "archive/artifacts/_dot" }
  ],
  "purgeDirs": ["dist","coverage","playwright","playwright-report",".cache",".tmp","tmp",".pytest_cache","test-dist"],
  "purgeGlobs": ["**/*.log","**/*.tmp","**/*.swp","**/.DS_Store","**/*~"],
  "keepRoots": ["apps","libs","tools","tests","py","python","scripts","docs",".github","schemas","export","archive","sbom","datasets","config"],
  "keepFiles": ["README.md","CHANGELOG.md","HOWTO-RUN.md","MAINTENANCE.md","SECURITY.md","STRUCTURE.md","CODEOWNERS","pnpm-workspace.yaml","pnpm-lock.yaml","package.json",".editorconfig",".eslintrc.json",".eslintignore",".prettierrc",".gitignore",".gitattributes",".gitleaks.toml",".hintc","tsconfig.json","tsconfig.base.json","tsconfig.test.json","stryker.conf.json","knip.json","knip-ignore.json","tsprune.json","tsprune-ignore.txt",".cspell.json"]
}
